{
  "id": "layout",
  "label": "layout",
  "format": "custom",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "logo_title": {
        "type": "Text",
        "config": {
          "label": "Logo Title",
          "placeholder": "Logo Title"
        }
      },
      "social_icons": {
        "type": "Group",
        "config": {
          "label": "Social Icons",
          "fields": {
            "social_icon_type": {
              "type": "Select",
              "config": {
                "label": "social_icon_type",
                "placeholder": "",
                "options": ["github", "linkedin", "email"],
                "default_value": "github"
              }
            },
            "link": {
              "type": "Link",
              "config": {
                "label": "link",
                "placeholder": "",
                "select": null
              }
            }
          }
        }
      },
      "contact_title": {
        "type": "Text",
        "config": {
          "label": "Contact Title",
          "placeholder": ""
        }
      },
      "contact_text": {
        "type": "StructuredText",
        "config": {
          "label": "Contact Text",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "contact_form": {
        "type": "Group",
        "config": {
          "label": "Contact Form",
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "name",
                "placeholder": ""
              }
            },
            "email": {
              "type": "Text",
              "config": {
                "label": "email",
                "placeholder": ""
              }
            },
            "message": {
              "type": "Text",
              "config": {
                "label": "message",
                "placeholder": ""
              }
            },
            "button": {
              "type": "Text",
              "config": {
                "label": "button",
                "placeholder": ""
              }
            }
          }
        }
      }
    }
  }
}
